    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "dappy-cli": "0.4.9",
    "rchain-token": "fabcotech/rchain-token#c01e1e5bd343c9cf2a7b1af8133541017daedb65",
    "react": "16.8.5",
    "react-dom": "16.8.5"
