{
  "name": "dapp-tipboard",
  "version": "1.0.4",
  "description": "Web application for Dappy to raise funds or create a jackpot.",
  "main": "index.js",
  "scripts": {
    "start": "webpack --mode development --watch",
    "build": "webpack --mode production",
